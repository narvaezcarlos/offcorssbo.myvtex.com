		"props": {
			"id": "banner-secundario",
			"isFullModeStyle": false,
			"textPosition": "center",
			"imageUrl": "/arquivos/banner-secundario-basicos-20.png",
			"mobileImageUrl": "/arquivos/banner-secundario-basicos-20.png",
			"callToActionText": "BUY",
