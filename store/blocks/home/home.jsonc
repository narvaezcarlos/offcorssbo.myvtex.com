{
	"store.home": {
		"blocks": [
			//"modal-trigger#home",
			//"flex-layout.row#topbanner",
			//"list-context.image-list#banner-principal",
			//"flex-layout.row#vitrina-basicos",
			//"flex-layout.row#vitrina-swimwear-home",
			//"flex-layout.row#vitrina-newborn",
			"rich-text#ejemplo"
		],
		"title": "Store init"
	},
	"flex-layout.row#topbanner": {
		"children": [
			"rich-text#topbanner-dark"
		],
		"props": {
			"blockClass": "topbanner__row",
			"colGap": 3
		}
	},

	"rich-text#ejemplo":{
		"props": {
			"text": "esto se va a descontrolar"
		}
	},


	"rich-text#topbanner-dark": {
		"props": {
			"textAlignment": "CENTER",
			"textPosition": "CENTER",
			"text": "### **FREE SHIPPING ON ALL ORDERS!** Available for limited time. ",
			"blockClass": "topbanner__dark"
		}
	},

	/*-----------------vitrina basicos Home-------------*/
	"flex-layout.row#vitrina-basicos": {
		"children": [
			"flex-layout.col#vitrina-basicos"
		],"props": {
			"blockClass": "vitrina1-producto-semana"
		}
	},
	"flex-layout.col#vitrina-basicos": {
		"children": [
			"flex-layout.row#richtext-home",
			"list-context.product-list#vitrina-basicos"
		]
	},

	"flex-layout.row#richtext-home":{
		"children": [
			"rich-text#vitrina-basicos",
			"rich-text#see-all"
		],"props": {
			"blockClass": "richtext__container"
		}
	},
	"rich-text#vitrina-basicos": {
		"props": {
			"textAlignment": "LEFT",
			"textPosition": "LEFT",
			"text": "THERE ARE LOOKS INSPIRED BY EVERYDAY ADVENTURES!",
			"textColor": "black-90",
			"font": "t-heading-3",
			"blockClass": "home-title"
		}
	},
	"rich-text#see-all": {
		"props": {
			"textAlignment": "RIGHT",
			"textPosition": "RIGHT",
			"text": "[SEE ALL](/262?map=productClusterIds)",
			"textColor": "black-90",
			"font": "t-heading-3",
			"blockClass": "home-title__seeall"
		}
	},
	"list-context.product-list#vitrina-basicos": {
		"blocks": ["product-summary.shelf"],
		"children": ["slider-layout#vitrina-home-testab"],
		"props": {
			"orderBy": "OrderByPriceDESC",
			"hideUnavailableItems": true,
			"collection": "262",
			"blockClass": "novedades-contet"
		}
	},

	"slider-layout#vitrina-home-testab": {
		"props": {
			"itemsPerPage": {
				"desktop": 5,
				"tablet": 3,
				"phone": 2
			},
			"infinite": true,
			"fullWidth": false,
			"blockClass": "shelf",
			"showPaginationDots": "never"
		}
	},
	/*-----------------vitrina swimwear Home-------------*/
	"flex-layout.row#vitrina-swimwear-home": {
		"children": [
			"flex-layout.col#vitrina-swimwear-home"
		],"props": {
			"blockClass": "vitrina1-producto-semana"
		}
	},
	"flex-layout.col#vitrina-swimwear-home": {
		"children": [
			"flex-layout.row#richtext-swimwear-home",
			"list-context.product-list#vitrina-swimwear-home"
		]
	},

	"flex-layout.row#richtext-swimwear-home":{
		"children": [
			"rich-text#vitrina-swimwear-home",
			"rich-text#see-all-swimwear-home"
		],"props": {
			"blockClass": "richtext__container"
		}
	},
	"rich-text#vitrina-swimwear-home": {
		"props": {
			"textAlignment": "LEFT",
			"textPosition": "LEFT",
			"text": "SUMMER IS THE MOST FUN SEASON AND DESERVES A COOL WARDROVE!",
			"textColor": "black-90",
			"font": "t-heading-3",
			"blockClass": "home-title"
		}
	},
	"rich-text#see-all-swimwear-home": {
		"props": {
			"textAlignment": "RIGHT",
			"textPosition": "RIGHT",
			"text": "[SEE ALL](/266?map=productClusterIds)",
			"textColor": "black-90",
			"font": "t-heading-3",
			"blockClass": "home-title__seeall"
		}
	},
	"list-context.product-list#vitrina-swimwear-home": {
		"blocks": ["product-summary.shelf"],
		"children": ["slider-layout#vitrina-home-testab"],
		"props": {
			"orderBy": "OrderByPriceDESC",
			"hideUnavailableItems": true,
			"collection": "266",
			"blockClass": "novedades-contet"
		}
	},

	/*-----------------vitrina newborn Home-------------*/
	"flex-layout.row#vitrina-newborn": {
		"children": [
			"flex-layout.col#vitrina-newborn"
		]
	},
	"flex-layout.col#vitrina-newborn": {
