		"children": [
			"rich-text#vitrina-newborn",
			"rich-text#see-all-newborn"
		],"props": {
			"blockClass": "richtext__container"
		}
	},
	"rich-text#vitrina-newborn": {
		"props": {
			"textAlignment": "LEFT",
			"textPosition": "LEFT",
			"text": "DISCOVER HOW WE ENJOY EVERY STAGE OF LIFE!",
			"textColor": "black-90",
			"font": "t-heading-3",
			"blockClass": "home-title"
		}
	},
	"rich-text#see-all-newborn": {
		"props": {
			"textAlignment": "RIGHT",
